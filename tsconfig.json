{
  "compilerOptions": {
    "target": "es2016", 
    "experimentalDecorators": true, 
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "./build",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true  
  }
}
